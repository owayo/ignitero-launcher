    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "wanakana": "^5.3.1"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.9.2",
    "@types/react": "^18.3.1f9",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^5.1.0",
    "prettier": "^3.5.3",
